                  <Font name="Algerian" size="15.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="60.0" fitWidth="77.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Img/2.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button mnemonicParsing="false" onAction="#back" prefHeight="83.0" prefWidth="238.0" text="Back">
               <font>
                  <Font name="Algerian" size="20.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <TableView fx:id="foodTable" layoutY="116.0" prefHeight="451.0" prefWidth="673.0">
        <columns>
          <TableColumn fx:id="foodName" prefWidth="334.0" text="Food Name" />
          <TableColumn fx:id="foodPrice" prefWidth="338.0" text="Price" />
        </columns>
      </TableView>
      <Text layoutX="294.0" layoutY="104.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Food LIst">
         <font>
            <Font name="Algerian" size="15.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
