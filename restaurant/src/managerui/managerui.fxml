<AnchorPane id="AnchorPane" fx:id="foodlist" prefHeight="566.0" prefWidth="668.0" style="-fx-background-color: green;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="managerui.ManageruiController">
   <children>
      <HBox layoutX="-2.0" prefHeight="83.0" prefWidth="673.0">
         <children>
            <Button mnemonicParsing="false" onAction="#add_del_but_method" prefHeight="83.0" prefWidth="291.0" text="ADD/Delete Food Item">
               <font>
                  <Font name="Algerian" size="15.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="51.0" fitWidth="71.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Img/add-image.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button mnemonicParsing="false" onAction="#customerlist_but_method" prefHeight="83.0" prefWidth="253.0" text="Cutomer List">
